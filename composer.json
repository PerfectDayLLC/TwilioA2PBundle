{
    "name": "perfectdayllc/twilio-a2p-bundle",
    "description": "Usage for Twilio A2P 10-DLC Integrations",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Matias Hernan Lauriti",
            "email": "matias.lauriti@rockstarcoders.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/bus": "^5.8|^6|^7|^8|^9",
        "illuminate/console": "^5.8|^6|^7|^8|^9",
        "illuminate/contracts": "^5.8|^6|^7|^8|^9",
        "illuminate/database": "^5.8|^6|^7|^8|^9",
        "illuminate/queue": "^5.8|^6|^7|^8|^9",
        "illuminate/support": "^5.8|^6|^7|^8|^9",
        "twilio/sdk": "^6.15"
    },
    "require-dev": {
        "doctrine/dbal": "^2.13.3|^3.1.2",
        "laravel/legacy-factories": "^1.1",
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^3.8|^4|^5|^6|^7",
        "phpunit/phpunit": "^8.0|^9.5"
    },
    "autoload": {
        "psr-4": {
            "PerfectDayLlc\\TwilioA2PBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/helpers.php"
        ],
        "psr-4": {
            "PerfectDayLlc\\TwilioA2PBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PerfectDayLlc\\TwilioA2PBundle\\TwilioA2PBundleServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
